{
    "name": "CodeMana",
    "version": "1.0.0",
    "description": "Enhance GitHub Gists",
    "dependencies": {
        "babel-preset-react": "^6.5.0",
        "babelify": "^7.2.0",
        "browserify": "latest",
        "node-sass": "^3.4.2",
        "react": "latest",
        "react-dom": "latest",
        "react-modal": "latest",
        "react-router": "latest",
        "uglifyjs": "latest",
        "watchify": "latest",
        "whatwg-fetch": "^0.10.0"
    },
    "scripts": {
        "build-dev-js": "node_modules/.bin/browserify -t [ babelify --presets [ react ] ] components/*.js -o js/script.js",
        "build-prod-js": "node_modules/.bin/browserify -t [ babelify --presets [ react ] ] components/*.js | node_modules/.bin/uglifyjs > js/script.js",
        "build-dev-css": "node-sass --include-path scss scss/style.scss   css/style.css",
        "watch-js": "node_modules/.bin/watchify -t [ babelify --presets [ react ] ] components/*.js -o js/script.js",
        "server": "php -S localhost:8000 -t ./",
        "clean": "rm js/script.js public/css/style.css"
    },
    "author": "Jim Saunders",
    "license": "MIT"
}
