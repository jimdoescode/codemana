{
  "name": "CodeMana",
  "version": "1.0.0",
  "description": "Enhance GitHub Gists",
  "main": "index.js",
  "dependencies": {
    "browserify": "latest",
    "reactify": "latest",
    "watchify": "latest",
    "uglifyjs": "latest",
    "qwest": "^2.0.3",
    "react": "^0.13.3",
    "react-async": "^2.1.0",
    "react-modal": "^0.3.0",
    "react-router-component": "^0.26.0"
  },
  "scripts": {
    "build-dev": "node_modules/.bin/browserify -t reactify app/components/*.js -o js/script.js",
    "build-prod": "node_modules/.bin/browserify -t reactify app/components/*.js | node_modules/.bin/uglifyjs > js/script.js",
    "watch": "node_modules/.bin/watchify -t reactify app/components/*.js -o js/script.js",
    "server": "php -S 0.0.0.0:8080",
    "clean-js": "rm js/script.js"
  },
  "author": "Jim Saunders",
  "license": "MIT"
}
