{
  "name": "CodeMana",
  "version": "1.0.0",
  "description": "Enhance GitHub Gists",
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "marked": "^0.3.6",
    "node-sass": "^3.13.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-modal": "^1.6.5",
    "react-redux": "^5.0.2",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0",
    "whatwg-fetch": "^0.10.1"
  },
  "scripts": {
    "build-dev-js": "node_modules/.bin/browserify -t [ babelify --presets [ es2015 react ] ] components/*.js -o public/js/script.js",
    "build-prod-js": "NODE_ENV=production node_modules/.bin/browserify -t [ babelify --presets [ es2015 react ] ] -g uglifyify components/*.js > public/js/script.js",
    "build-dev-css": "node_modules/.bin/node-sass ./scss/style.scss --output=./public/css",
    "build-prod-css": "NODE_ENV=production node_modules/.bin/node-sass ./scss/style.scss --output-style=compressed --output=./public/css"
    "watch-js": "node_modules/.bin/watchify -t [ babelify --presets [ react ] ] components/*.js -o public/js/script.js",
    "start": "php -S 0.0.0.0:8000 -t ./public/",
    "clean": "rm public/js/script.js public/css/style.css"
  },
  "author": "Jim Saunders",
  "license": "MIT",
  "repository": "https://github.com/jimdoescode/codemana"
}
